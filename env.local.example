# ===========================================
# Land Sales Dashboard - Environment Variables
# ===========================================
# Copy this file to .env.local and fill in the values

# Database (Local PostgreSQL)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/land_sales

# Supabase (Optional - for storage and auth)
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=
# SUPABASE_SERVICE_ROLE_KEY=

# Storage Buckets (if using Supabase)
SUPABASE_STORAGE_BUCKET_MANUAL=manual-uploads
SUPABASE_STORAGE_BUCKET_AI=ai-generated-content

# Facebook/Meta (Optional)
FB_APP_ID=
FB_APP_SECRET=
FB_REDIRECT_URI=http://localhost:3000/api/facebook/callback

# n8n (Optional)
N8N_WEBHOOK_URL=
N8N_API_KEY=

# AI Services (Optional)
GEMINI_API_KEY=
VEO3_API_KEY=

# Application Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
